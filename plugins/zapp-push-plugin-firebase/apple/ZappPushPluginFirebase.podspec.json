{
  "name": "ZappPushPluginFirebase",
  "version": "2.0.0",
  "summary": "FirebasePushPlugin for Zapp iOS.",
  "description": "FirebasePushPlugin for Zapp iOS.",
  "homepage": "https://github.com/applicaster/Zapp-Frameworks.git",
  "license": "CMPS",
  "authors": "Applicaster LTD.",
  "source": {
    "git": "https://github.com/applicaster/Zapp-Frameworks.git",
    "tag": "2.0.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "requires_arc": true,
  "swift_versions": "5.1",
  "source_files": "ios/**/*.{swift}",
  "default_subspecs": "Base",
  "static_framework": true,
  "prepare_command": "sh Scripts/prepare_service_extension.sh\nsh Scripts/prepare_content_extension.sh",
  "subspecs": [
    {
      "name": "Base",
      "xcconfig": {
        "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
        "FRAMEWORK_SEARCH_PATHS": "$(inherited)",
        "OTHER_LDFLAGS": "$(inherited)",
        "ENABLE_BITCODE": "YES",
        "OTHER_CFLAGS": "-fembed-bitcode"
      },
      "dependencies": {
        "ZappPushPluginsSDK": [],
        "ZappPlugins": [],
        "Firebase/Messaging": [],
        "Firebase/Core": []
      }
    },
    {
      "name": "ServiceExtension",
      "xcconfig": {
        "SWIFT_VERSION": "5.1",
        "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
        "ENABLE_BITCODE": "YES",
        "OTHER_CFLAGS": "-fembed-bitcode"
      },
      "dependencies": {
        "Firebase": [],
        "Firebase/Messaging": []
      }
    },
    {
      "name": "ContentExtension",
      "xcconfig": {
        "SWIFT_VERSION": "5.1",
        "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
        "ENABLE_BITCODE": "YES",
        "OTHER_CFLAGS": "-fembed-bitcode"
      },
      "dependencies": {
        "Firebase": [],
        "Firebase/Messaging": []
      }
    }
  ],
  "testspecs": [
    {
      "name": "UnitTests",
      "test_type": "unit",
      "source_files": "tests/**"
    }
  ],
  "swift_version": "5.1"
}
