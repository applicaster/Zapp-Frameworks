{
  "name": "QuickBrickXray",
  "version": "1.2.0",
  "platforms": {
    "ios": "12.0"
  },
  "summary": "QuickBrickXray",
  "description": "QuickBrickXray.",
  "homepage": "https://github.com/applicaster/Zapp-Frameworks.git",
  "license": "Appache 2.0",
  "authors": "Applicaster LTD.",
  "source": {
    "git": "https://github.com/applicaster/Zapp-Frameworks.git",
    "tag": "@@applicaster/quick-brick-xray/1.2.0"
  },
  "requires_arc": true,
  "swift_versions": "5.1",
  "source_files": "universal/**/*.swift",
  "ios": {
    "source_files": "ios/**/*.swift"
  },
  "resources": [
    "universal/**/*.{storyboard}"
  ],
  "xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "ENABLE_BITCODE": "YES"
  },
  "dependencies": {
    "ZappCore": [],
    "XrayLogger": [],
    "XrayLogger/ReactNative": [],
    "Reporter": [],
    "LoggerInfo": []
  },
  "testspecs": [
    {
      "name": "UnitTests",
      "test_type": "unit",
      "ios": {
        "source_files": "tests/**/*.{swift}",
        "resources": [
          "tests/iOS/**/*.{png}"
        ]
      },
      "tvos": {
        "source_files": "tests/**/Dummy.swift"
      }
    }
  ],
  "swift_version": "5.1"
}