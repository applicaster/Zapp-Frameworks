{
  "name": "ZappGoogleInteractiveMediaAds",
  "version": "0.13.0",
  "swift_versions": "5.1",
  "summary": "ZappGoogleInteractiveMediaAds",
  "description": "This plugin allow to add Google Interactive Media Ads to supported players.",
  "homepage": "https://github.com/applicaster/AppleApplicasterFrameworks.git",
  "license": "Appache 2.0",
  "authors": "Applicaster LTD.",
  "source": {
    "git": "https://github.com/applicaster/Zapp-Frameworks.git",
    "tag": "@@applicaster/quick-brick-google-ima-apple/0.13.0"
  },
  "platforms": {
    "ios": "11.0",
    "tvos": "11.0"
  },
  "tvos": {
    "source_files": "universal/**/*.swift",
    "dependencies": {
      "GoogleAds-IMA-tvOS-SDK": [
        "= 4.3.2"
      ]
    }
  },
  "ios": {
    "source_files": "universal/**/*.swift",
    "dependencies": {
      "GoogleAds-IMA-iOS-SDK": [
        "= 3.12.1"
      ]
    }
  },
  "dependencies": {
    "ZappCore": []
  },
  "xcconfig": {
    "ENABLE_BITCODE": "YES",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "ENABLE_TESTABILITY": "YES",
    "OTHER_CFLAGS": "-fembed-bitcode"
  },
  "testspecs": [
    {
      "name": "UnitTests",
      "test_type": "unit",
      "source_files": "Tests/**"
    }
  ],
  "swift_version": "5.1"
}