{
  "name": "ZappTHEOplayer",
  "version": "0.5.0",
  "summary": "THEOplayer",
  "platforms": {
    "ios": "12.0",
    "tvos": "12.0"
  },
  "swift_versions": "5.3",
  "description": "THEOplayer",
  "homepage": "https://github.com/applicaster/AppleApplicasterFrameworks.git",
  "license": "Appache 2.0",
  "authors": {
    "a.kononenko@applicaster.com": "a.kononenko@applicaster.com"
  },
  "source": {
    "git": "https://github.com/applicaster/Zapp-Frameworks",
    "tag": "@@applicaster/zapp-react-native-theo-player/0.5.0"
  },
  "static_framework": true,
  "ios": {
    "xcconfig": {
      "ENABLE_BITCODE": "YES",
      "OTHER_CFLAGS": "-fembed-bitcode"
    },
    "dependencies": {
      "GoogleAds-IMA-iOS-SDK": [
        "= 3.13.0"
      ],
      "React": [],
      "ZappCore": [],
      "google-cast-sdk": [
        "= 4.3.0"
      ]
    },
    "source_files": "ios/**/*.{swift}"
  },
  "tvos": {
    "xcconfig": {
      "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
      "ENABLE_BITCODE": "YES",
      "OTHER_CFLAGS": "-fembed-bitcode"
    },
    "source_files": "tvos/**/*.{swift}"
  },
  "vendored_frameworks": [
    "THEOplayerSDK.xcframework"
  ],
  "dependencies": {
    "XrayLogger": []
  },
  "source_files": [
    "universal/**/*.{h,m,swift}"
  ],
  "resources": [
    "ui_customization/**/*"
  ],
  "testspecs": [
    {
      "name": "UnitTests",
      "test_type": "unit",
      "source_files": "Tests/**"
    }
  ],
  "swift_version": "5.1"
}