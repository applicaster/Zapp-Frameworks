  "ui_frameworks": [
    "quickbrick"
  ],
  "preload": true,
  "custom_configuration_fields": [],
  "hooks": {
    "fields": [
      {
        "group": true,
        "label": "Before Load",
        "folded": true,
        "fields": [
          {
            "key": "preload_plugins",
            "type": "preload_plugins_selector",
            "label": "Select Plugins"
          }
        ]
      }
    ]
  },
  "platform": "android_for_quickbrick",
  "dependency_version": "0.3.0",
  "manifest_version": "0.3.0",
  "min_zapp_sdk": "0.1.0-alpha1",
  "styles": {
